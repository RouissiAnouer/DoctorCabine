<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.halgo.clientcab">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".InscriptionActivity"/>
        <activity android:name=".VerificationPass"/>
        <activity android:name=".NouveauRendezVous"/>
        <activity android:name=".DetailDateRDV"/>
        <activity android:name=".ListRendezVousActivity"/>
        <activity android:name=".ListViewExemple"/>
        <activity android:name=".ListRowActivity"/>
        <activity android:name=".DetailHeureRDV"/>
    </application>

</manifest>